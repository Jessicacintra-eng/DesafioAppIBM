<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".paginicial">

  <ImageView
      android:id="@+id/imageView3"
      android:layout_width="410dp"
      android:layout_height="68dp"
      android:background="@color/purple_200"
      android:backgroundTint="@color/purple_200"
      android:foregroundTint="@color/purple_200"
      android:visibility="visible"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      tools:visibility="visible" />

  <ImageView
      android:id="@+id/hamburgfechado"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      android:layout_marginBottom="13dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView3"
      app:layout_constraintEnd_toEndOf="@+id/imageView3"
      app:layout_constraintHorizontal_bias="0.046"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/ic_closed_hamburguer" />

  <ImageView
      android:id="@+id/imageView6"
      android:layout_width="119dp"
      android:layout_height="37dp"
      android:layout_marginTop="20dp"
      android:layout_marginBottom="16dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView3"
      app:layout_constraintEnd_toEndOf="@+id/imageView3"
      app:layout_constraintHorizontal_bias="0.928"
      app:layout_constraintStart_toEndOf="@+id/hamburgfechado"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.4"
      app:srcCompat="@drawable/ic_ibm_yellow" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="348dp"
      android:layout_height="418dp"
      android:layout_marginBottom="64dp"
      android:background="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.492"
      app:layout_constraintStart_toStartOf="parent"
      app:srcCompat="@drawable/caixa" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="95dp"
      android:layout_height="26dp"
      android:layout_marginStart="24dp"
      android:layout_marginTop="24dp"
      android:fontFamily="@font/ibm_plex_sans_medium"
      android:text="Welcome,"
      android:textSize="20sp"
      app:layout_constraintBottom_toTopOf="@+id/textView7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.375"
      app:layout_constraintStart_toEndOf="@+id/imageView8"
      app:layout_constraintTop_toBottomOf="@+id/imageView3"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="113dp"
      android:layout_height="29dp"
      android:layout_marginStart="60dp"
      android:layout_marginBottom="20dp"
      android:fontFamily="@font/ibm_plex_sans_medium"
      android:text="SEUNOME"
      android:textSize="24sp"
      app:layout_constraintBottom_toTopOf="@+id/imageView7"
      app:layout_constraintStart_toEndOf="@+id/imageView8" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="87dp"
      android:layout_height="89dp"
      android:layout_marginTop="8dp"
      app:layout_constraintBottom_toTopOf="@+id/imageView7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.246"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView3"
      app:layout_constraintVertical_bias="0.25"
      app:srcCompat="@drawable/avatar" />

</androidx.constraintlayout.widget.ConstraintLayout>